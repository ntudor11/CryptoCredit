#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/tudornastasa/Main/webDev/uni/CryptoCredit08/node_modules/.pnpm/nodemon@1.19.4/node_modules/nodemon/bin/nodemon.js/node_modules:/Users/tudornastasa/Main/webDev/uni/CryptoCredit08/node_modules/.pnpm/nodemon@1.19.4/node_modules/nodemon/bin/node_modules:/Users/tudornastasa/Main/webDev/uni/CryptoCredit08/node_modules/.pnpm/nodemon@1.19.4/node_modules/nodemon/node_modules:/Users/tudornastasa/Main/webDev/uni/CryptoCredit08/node_modules/.pnpm/nodemon@1.19.4/node_modules:/Users/tudornastasa/Main/webDev/uni/CryptoCredit08/node_modules/.pnpm/node_modules:/Users/tudornastasa/Main/webDev/uni/CryptoCredit08/node_modules:/Users/tudornastasa/Main/webDev/uni/node_modules:/Users/tudornastasa/Main/webDev/node_modules:/Users/tudornastasa/Main/node_modules:/Users/tudornastasa/node_modules:/Users/node_modules:/node_modules:/Users/tudornastasa/Main/webDev/uni/CryptoCredit08/node_modules/nodemon/bin/nodemon.js/node_modules:/Users/tudornastasa/Main/webDev/uni/CryptoCredit08/node_modules/nodemon/bin/node_modules:/Users/tudornastasa/Main/webDev/uni/CryptoCredit08/node_modules/nodemon/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nodemon/bin/nodemon.js" "$@"
else 
  exec node  "$basedir/../nodemon/bin/nodemon.js" "$@"
fi
